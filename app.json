{
  "expo": {
    "name": "appCalendario",
    "slug": "appCalendario",
    "version": "1.0.0",
    "scheme": "appCalendario",
    "linking": {
      "prefixes": ["exp://", "appCalendario://"],
      "config": {
        "screens": {
          "novaSenha": "novaSenha/:token"
        }
      }
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.appCalendario",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.appCalendario"
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
